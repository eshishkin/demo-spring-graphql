input OrderRequest {
    description: String!
    email: String!
    items: [OrderItemRequest]!
}

input OrderItemRequest {
    code: String!
    amount: Int!
}

type Order {
    id: Int
    description: String
    state: String
    customer: Customer
    items: [Item]
    created: String
}

type Item {
    code: String
    title: String
    amount: Int
    merchant: Merchant
}